{
  "name": "react-express-app",
  "version": "0.4.0",
  "description": "CLI",
  "main": "index.js",
  "bin": {
    "react-express-app": "index.js"
  },
  "scripts": {
    "publish": "npm publish",
    "lint": "eslint .",
    "format": "prettier-standard --lint \"**/*.{js,jsx,ts,json}\"",
    "valite": "npm run format && npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cli",
    "react-express-app",
    "boilerplate",
    "react",
    "redux",
    "hooks",
    "builder"
  ],
  "author": {
    "name": "Jesus Hernandez",
    "url": "https://www.jesuhrz.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JesuHrz/react-express-app.git"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "ejs": "^3.1.3",
    "execa": "^4.0.2",
    "figlet": "^1.4.0",
    "file-extension": "^4.0.5",
    "husky": "^4.2.5",
    "inquirer": "^7.2.0",
    "listr": "^0.14.3",
    "minimist": "^1.2.5",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-ejs": "0.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "lint-staged": "^10.2.11",
    "prettier-standard": "^16.3.0"
  },
  "lint-staged": {
    "*.{json}": [
      "prettier-standard '**/*.{json}'"
    ],
    "*.{js,jsx,ts}": [
      "prettier-standard --lint \"**/*.{js,jsx,ts,json}\"",
      "eslint ."
    ]
  },
  "license": "MIT"
}
